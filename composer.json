{
  "name" : "luka/magento-core-installer",
  "description" : "Magento Core installation via Composer",
  "require" : {
    "composer-plugin-api" : "^1.0.0",
    "magento-hackathon/magento-composer-installer" : "*"
  },
  "authors" : [ {
    "name" : "Zvonimir Buric",
    "email" : "zvonimir.buric@luka.de"
  }, {
    "name" : "Axel Helmert",
    "email" : "ah@luka.de"
  } ],
  "keywords" : [ "composer-installer", "magento" ],
  "minimum-stability" : "dev",
  "extra" : {
    "class" : "Luka\\Composer\\MagentoCore\\Installer",
    "branch-alias" : {
      "dev-master" : "1.0.x-dev"
    }
  },
  "autoload" : {
    "psr-0" : {
      "Luka\\Composer\\MagentoCore" : "src/"
    }
  },
  "license" : "MIT",
  "type" : "composer-plugin"
}
